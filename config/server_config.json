                "enabled": false,
                "time": "08:00",
                "name": "",
                "weather_location": "",
                "include_schedule": true,
                "include_fun": false,
                "send_if_weekend": true,
                "preferred_variant": 2,
                "subject_overrides": [
                    {
                        "pattern": "AMiAL, cw",
