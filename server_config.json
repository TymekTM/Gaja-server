{
  "server": {
    "host": "0.0.0.0",
    "port": 8001,
    "debug": false,
    "workers": 1,
    "reload": false
  },
  "database": {
    "url": "sqlite:///./server_data.db",
    "echo": false
  },
  "ai": {
    "provider": "openai",
    "model": "gpt-5-nano",
    "api_key": "",
    "max_tokens": 1500,
    "temperature": 0.7
  },
  "auth": {
    "secret_key": "your-secret-key-change-this",
    "algorithm": "HS256",
    "access_token_expire_minutes": 60
  },
  "features": {
    "memory_enabled": true,
    "function_calling": true,
    "proactive_assistant": false,
    "web_search": true
  },
  "paths": {
    "data_dir": "./data",
    "logs_dir": "./logs",
    "cache_dir": "./cache"
  }
}