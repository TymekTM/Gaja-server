{
    "server": {
        "host": "0.0.0.0",
        "port": 8000,
        "debug": false
    },
    "database": {
        "url": "sqlite:///./server_data.db",
        "echo": false
    },
    "ai": {
        "provider": "openrouter",
        "model": "openai/gpt-oss-120b",
        "temperature": 0.7,
        "max_tokens": 1000,
        "provider_models": {
            "openrouter": "openai/gpt-oss-120b",
            "openai": "gpt-5-nano"
        }
    },
    "api_keys": {
        "openai": "YOUR_OPENAI_API_KEY_HERE",
        "anthropic": "YOUR_ANTHROPIC_API_KEY_HERE",
        "openrouter": "YOUR_OPENROUTER_API_KEY_HERE"
    },
    "plugins": {
        "auto_load": true,
        "default_enabled": [
            "weather_module",
            "search_module"
        ]
    },
    "logging": {
        "level": "INFO",
        "file": "logs/server_{time:YYYY-MM-DD}.log"
    },
    "ui_language": "en"
}
